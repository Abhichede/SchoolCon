<%- model_class = StudentWiseFine -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:student_id) %>:</strong></dt>
  <dd><%= @student_wise_fine.student_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:academic_year_id) %>:</strong></dt>
  <dd><%= @student_wise_fine.academic_year_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:desciption) %>:</strong></dt>
  <dd><%= @student_wise_fine.desciption %></dd>
  <dt><strong><%= model_class.human_attribute_name(:amount) %>:</strong></dt>
  <dd><%= @student_wise_fine.amount %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              student_wise_fines_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_student_wise_fine_path(@student_wise_fine), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              student_wise_fine_path(@student_wise_fine),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
