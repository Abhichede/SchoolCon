<%- model_class = TimeTableSetting -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:school_start) %></th>
      <th><%= model_class.human_attribute_name(:school_end) %></th>
      <th><%= model_class.human_attribute_name(:period_duration) %></th>
      <th><%= model_class.human_attribute_name(:num_breaks) %></th>
      <th><%= model_class.human_attribute_name(:break_durations) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @time_table_settings.each do |time_table_setting| %>
      <tr>
        <td><%= link_to time_table_setting.id, time_table_setting_path(time_table_setting) %></td>
        <td><%= time_table_setting.school_start %></td>
        <td><%= time_table_setting.school_end %></td>
        <td><%= time_table_setting.period_duration %></td>
        <td><%= time_table_setting.num_breaks %></td>
        <td><%= time_table_setting.break_durations %></td>
        <td><%=l time_table_setting.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      time_table_setting_path(time_table_setting), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_time_table_setting_path(time_table_setting), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      time_table_setting_path(time_table_setting),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_time_table_setting_path,
            :class => 'btn btn-primary' %>
