<%- model_class = StudentWiseInstantFee -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:student_id) %>:</strong></dt>
  <dd><%= @student_wise_instant_fee.student_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:academic_year_id) %>:</strong></dt>
  <dd><%= @student_wise_instant_fee.academic_year_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @student_wise_instant_fee.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:amount) %>:</strong></dt>
  <dd><%= @student_wise_instant_fee.amount %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              student_wise_instant_fees_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_student_wise_instant_fee_path(@student_wise_instant_fee), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              student_wise_instant_fee_path(@student_wise_instant_fee),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
